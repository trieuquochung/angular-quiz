# Firebase Configuration Template
# Copy this to .env and replace with your actual values
# Get these values from: https://console.firebase.google.com/project/YOUR_PROJECT/settings/general

NG_APP_FIREBASE_API_KEY=your-api-key-here
NG_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
NG_APP_FIREBASE_PROJECT_ID=your-project-id
NG_APP_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
NG_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
NG_APP_FIREBASE_APP_ID=1:123456789:web:your-app-id
NG_APP_FIREBASE_MEASUREMENT_ID=G-YOUR-MEASUREMENT-ID